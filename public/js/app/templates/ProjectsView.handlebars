<div class="span12 center-form">
    <button class="btn btn-success btn-small">Add Project</button>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {{#if Projects}}

            {{#each Projects}}
                <tr>
                    <td>{{id}}</td>
                    <td><a href="#project/{{id}}">{{name}}</a></td>
                    <td></td>
                </tr>
            {{/each}}
        {{/if}}
        </tbody>
    </table>
</div>